{
  "name": "tree-sitter-scala",
  "version": "0.23.2",
  "description": "Scala grammar for tree-sitter",
  "repository": "github:tree-sitter/tree-sitter-scala",
  "license": "MIT",
  "author": "Max Brunsfeld <maxbrunsfeld@gmail.com>",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
